<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
    <version>5.0</version>
    <date>2021-12-14T21:25:27Z</date>
    <groups>
        <group>
            <name>Raspberry Pi</name>
        </group>
    </groups>
    <templates>
        <template>
            <template>Template Raspberry Pi Hardware</template>
            <name>Template Raspberry Pi Hardware</name>
            <description>Collects inventory items</description>
            <groups>
                <group>
                    <name>Raspberry Pi</name>
                </group>
            </groups>
            <applications>
                <application>
                    <name>Inventory</name>
                </application>
            </applications>
            <items>
                <item>
                    <name>cpuinfo</name>
                    <key>rpi.cpu.info</key>
                    <delay>5m</delay>
                    <history>365d</history>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <applications>
                        <application>
                            <name>Inventory</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>DISCARD_UNCHANGED_HEARTBEAT</type>
                            <params>1d</params>
                        </step>
                    </preprocessing>
                </item>
                <item>
                    <name>CPU Model</name>
                    <type>DEPENDENT</type>
                    <key>rpi.cpu.model</key>
                    <delay>0</delay>
                    <history>365d</history>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <inventory_link>HARDWARE</inventory_link>
                    <applications>
                        <application>
                            <name>Inventory</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>REGEX</type>
                            <params>Hardware.*\: (.*)
\1</params>
                        </step>
                    </preprocessing>
                    <master_item>
                        <key>rpi.cpu.info</key>
                    </master_item>
                </item>
                <item>
                    <name>CPU Revision</name>
                    <type>DEPENDENT</type>
                    <key>rpi.cpu.revision</key>
                    <delay>0</delay>
                    <history>365d</history>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <applications>
                        <application>
                            <name>Inventory</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>REGEX</type>
                            <params>Revision.*\: (.*)
\1</params>
                        </step>
                    </preprocessing>
                    <master_item>
                        <key>rpi.cpu.info</key>
                    </master_item>
                </item>
                <item>
                    <name>Serial Number</name>
                    <type>DEPENDENT</type>
                    <key>rpi.cpu.serial</key>
                    <delay>0</delay>
                    <history>365d</history>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <inventory_link>SERIALNO_A</inventory_link>
                    <applications>
                        <application>
                            <name>Inventory</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>REGEX</type>
                            <params>Serial.*\: (.*)
\1</params>
                        </step>
                    </preprocessing>
                    <master_item>
                        <key>rpi.cpu.info</key>
                    </master_item>
                </item>
                <item>
                    <name>CPU Temperature</name>
                    <key>rpi.cpu.temperature</key>
                    <value_type>FLOAT</value_type>
                    <units>°C</units>
                    <triggers>
                        <trigger>
                            <expression>{avg(5m,0)}&gt;{$CPU.TEMP.HIGH}</expression>
                            <name>CPU Temperature High (over {$CPU.TEMP.HIGH}°C for 5m)</name>
                            <priority>HIGH</priority>
                        </trigger>
                        <trigger>
                            <expression>{avg(5m,0)}&gt;{$CPU.TEMP.WARN}</expression>
                            <name>CPU Temperature Warning (over {$CPU.TEMP.WARN}°C for 5m)</name>
                            <priority>WARNING</priority>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <name>Model</name>
                    <type>DEPENDENT</type>
                    <key>rpi.model</key>
                    <delay>0</delay>
                    <history>365d</history>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <inventory_link>MODEL</inventory_link>
                    <applications>
                        <application>
                            <name>Inventory</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>REGEX</type>
                            <params>Model.*\: (.*)
\1</params>
                        </step>
                    </preprocessing>
                    <master_item>
                        <key>rpi.cpu.info</key>
                    </master_item>
                </item>
            </items>
            <macros>
                <macro>
                    <macro>{$CPU.TEMP.HIGH}</macro>
                    <value>80</value>
                </macro>
                <macro>
                    <macro>{$CPU.TEMP.WARN}</macro>
                    <value>70</value>
                </macro>
            </macros>
        </template>
    </templates>
    <graphs>
        <graph>
            <name>CPU Temperature</name>
            <yaxismin>40</yaxismin>
            <ymin_type_1>FIXED</ymin_type_1>
            <ymax_type_1>FIXED</ymax_type_1>
            <graph_items>
                <graph_item>
                    <sortorder>1</sortorder>
                    <color>1A7C11</color>
                    <item>
                        <host>Template Raspberry Pi Hardware</host>
                        <key>rpi.cpu.temperature</key>
                    </item>
                </graph_item>
            </graph_items>
        </graph>
    </graphs>
</zabbix_export>
